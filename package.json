{
  "name": "my-store",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_DEV=dev nodemon index.js",
    "start": "node index.js",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "NODE_ENV=development sequelize-cli db:migrate",
    "migrations:run:test": "sequelize-cli db:migrate --env test",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:revert:test": "sequelize-cli db:migrate:undo --env test",
    "migrations:revert:test:hard": "sequelize-cli db:migrate:undo:all --env test",
    "swagger-autogen": "node ./swagger.js",
    "test": "jest --verbose --detectOpenHandles",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "faker": "^5.5.3",
    "i": "^0.3.7",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.2",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.16.1",
    "sequelize-cli": "^6.4.1",
    "swagger-autogen": "^2.19.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0"
  },
  "engines": {
    "node": "16.x"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "node_modules",
      "mysql_data",
      "postgres_data"
    ]
  }
}
